<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.album.SignupController">
   <children>
      <Label layoutX="221.0" layoutY="39.0" text="Signup">
         <font>
            <Font name="DejaVu Sans Mono Bold" size="37.0" />
         </font>
      </Label>
      <Label layoutX="126.0" layoutY="155.0" text="Username:">
         <font>
            <Font name="DejaVu Sans Mono Bold" size="18.0" />
         </font></Label>
      <Label layoutX="126.0" layoutY="215.0" text="Password:">
         <font>
            <Font name="DejaVu Sans Mono Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="signupusername" layoutX="270.0" layoutY="154.0" />
      <TextField fx:id="signuppassword" layoutX="270.0" layoutY="214.0" />
      <Button layoutX="239.0" layoutY="277.0" mnemonicParsing="false" onAction="#clickToSignup" text="Signup!" />
      <Button fx:id="backtologin_button" layoutX="379.0" layoutY="277.0" mnemonicParsing="false" onAction="#switchToLogin" text="Go back to login" />
   </children>
</AnchorPane>
